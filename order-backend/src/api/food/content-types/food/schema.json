{
  "kind": "collectionType",
  "collectionName": "foods",
  "info": {
    "singularName": "food",
    "pluralName": "foods",
    "displayName": "food"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "text",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "discount": {
      "type": "integer",
      "required": true
    },
    "rating": {
      "type": "decimal",
      "required": true
    },
    "img": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "foods"
    },
    "quantity": {
      "type": "integer"
    }
  }
}
